Host example
    HostName some.example.com
    User ubuntu
    IdentityFile ~/.ssh/id_rsa
    ForwardAgent yes
    port 2225
    ForwardX11 yes
    LocalForward 8080 wwwcache.example.com:8081
    Compression yes
    ForwardX11 yes
    DynamicForward 20022

# === Dynamic SOCKS Proxy ===
Host socksproxy
    HostName proxy.example.com
    User youruser
    Port 22
    DynamicForward 1080
    # Now you can use localhost:1080 as a SOCKS5 proxy

# === Local Port Forwarding ===
Host localforward
    HostName internal.example.com
    User youruser
    Port 22
    LocalForward 5432 127.0.0.1:5432
    # Forwards local port 5432 to remote 127.0.0.1:5432 (e.g., PostgreSQL)
    # Access remote DB as if it's local

# === Remote Port Forwarding ===
Host remoteforward
    HostName jump.example.com
    User youruser
    Port 22
    RemoteForward 8080 127.0.0.1:3000
    # Remote port 8080 forwards to your local 127.0.0.1:3000
    # Remote users can access your local web app

# === SSH Tunnel (Layer 3 VPN) ===
Host tunnelvpn
    HostName tun.example.com
    User youruser
    Port 22
    Tunnel yes
    TunnelDevice tun0:any
    # Creates a point-to-point tunnel interface (tun0)
    # Requires support on both ends and setup of routing rules

# === Agent Forwarding ===
Host forwardagent
    HostName dev.example.com
    User youruser
    Port 22
    ForwardAgent yes
    # Allows the remote machine to use your local SSH keys
    # Useful when hopping from server to server

# === X11 Forwarding ===
Host x11gui
    HostName gui.example.com
    User youruser
    Port 22
    ForwardX11 yes
    ForwardX11Trusted yes
    # Run GUI apps from remote server on your local machine
    # e.g., ssh x11gui && gedit

